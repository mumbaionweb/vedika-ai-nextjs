{
  "name": "vedika-ai-nextjs",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:turbo": "NODE_OPTIONS='--max-old-space-size=4096' next dev --turbopack",
    "dev:clean": "rm -rf .next && npm run dev",
