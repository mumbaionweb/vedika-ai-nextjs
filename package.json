    "@aws-sdk/credential-provider-cognito-identity": "^3.916.0",
    "@aws-sdk/signature-v4a": "^3.914.0",
    "ai": "^5.0.76",
    "lucide-react": "^0.546.0",
    "next": "^15.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
